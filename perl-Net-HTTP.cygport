CPAN_AUTHOR="gaas"
inherit perl

NAME="perl-Net-HTTP"
VERSION=6.06
RELEASE=1
CATEGORY="Perl"
SUMMARY="Low-level HTTP client module for Perl"
DESCRIPTION="The Net::HTTP class is a low-level HTTP client. An instance of the
Net::HTTP class represents a connection to an HTTP server. The HTTP protocol is
described in RFC 2616. The Net::HTTP class supports HTTP/1.0 and HTTP/1.1."
